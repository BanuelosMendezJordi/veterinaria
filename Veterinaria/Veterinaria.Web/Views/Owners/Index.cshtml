@model IEnumerable<Veterinaria.Web.Models.Owner>

@{
    ViewBag.Title = "Index";
}


<body style="background-image:url(../../Content/img/Interface/Paw_Wallpaper.jpg)">
    <div class="container">
        <div class="py-3"></div>
        <div class="card">
            <div class="card-header bg-dark text-white text-center">
                <div>
                    <h2>DUEÑOS</h2>
                    <a href="@Url.Action("Create","Owners")" class="btn btn-success">Anexar Dueño</a>
                </div>
            </div>
            <div class="card-body text-center">
                <table class="table">
                    <tr>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                                @Html.ActionLink("Delete", "Delete", new { id = item.id })
                            </td>
                        </tr>
                    }

                </table>

            </div>
        </div>
    </div>
</body>
